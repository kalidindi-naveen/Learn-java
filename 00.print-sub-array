public class Main
{
	public static void main(String[] args) {
	    int ar[]={2,4,6,8,10};
	    int n=ar.length;
	    for(int i=0;i<n;i++){
	        for(int j=i;j<n;j++){
	            for(int k=i;k<=j;k++){
	                System.out.print(ar[k]+" ");
	            }
	            System.out.println();
	        }
	        System.out.println();
	    }
	}
}

/*
output:- 
2 
2 4 
2 4 6 
2 4 6 8 
2 4 6 8 10 

4 
4 6 
4 6 8 
4 6 8 10 

6 
6 8 
6 8 10 

8 
8 10 

10
*/
